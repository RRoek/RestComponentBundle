{
    "name": "laposte/rest-component-bundle",
    "description": "Boîte noire permettant la génération rapide et dynamique d'APIs RESTFul HATEOAS",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Roy Roek",
            "email": "contact@lemonways.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/rest-bundle": "^2.3",
        "goodby/csv": "^1.3",
        "jms/serializer-bundle": "^2.1",
        "symfony/config": "~3.2",
        "symfony/console": "~3.2",
        "symfony/dependency-injection": "~3.2",
        "symfony/framework-bundle": "~3.2",
        "symfony/monolog-bundle": "^3.1",
        "symfony/yaml": "~3.2"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^6.2",
        "matthiasnoback/symfony-dependency-injection-test": "^2.1",
        "matthiasnoback/symfony-config-test": "^3.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "RRoek\\RestComponentBundle\\": "src/RRoek/RestComponentBundle"
        }
    }
}
